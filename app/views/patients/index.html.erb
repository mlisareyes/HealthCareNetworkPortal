<h1>All Patients:</h1>

<% if !nested_user %>
  <%= form_tag(patients_path, :method => "get", id: "search-form") do %>
  <%= text_field_tag :search, params[:search], placeholder: "Search Patients" %>
  <%= submit_tag "Search" %>
  <% end %>
  <% else %>
<% end %>
<br>
<% @patients.each do |p| %>
  <li><%= link_to p.name, patient_path(p) %></li>
<% end %>
